{
  "name": "pong",
  "version": "0.0.1",
  "description": "Simple pong clone made with p5.js and sockets.io",
  "main": "bin/app.js",
  "scripts": {
    "start": "node bin/app.js",
    "devstart": "clear; nodemon bin/app.js",
    "serverstart": "DEBUG=express-locallibrary-tutorial:* npm run devstart"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndreaTerenz/p5-pong.git"
  },
  "keywords": [
    "pong",
    "p5",
    "sockets"
  ],
  "author": "AndreaTerenz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AndreaTerenz/p5-pong/issues"
  },
  "homepage": "https://github.com/AndreaTerenz/p5-pong#readme",
  "dependencies": {
    "express": "^4.17.1",
    "seedrandom": "^3.0.5",
    "socket.io": "^3.0.4"
  },
  "devDependencies": {
    "eslint": "^7.15.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.6"
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      "public/*",
      "*.json",
      "README",
      "LICENSE"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js css html"
  }
}
